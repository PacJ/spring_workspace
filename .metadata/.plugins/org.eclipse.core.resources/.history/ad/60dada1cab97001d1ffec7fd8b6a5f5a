package part02;

import java.util.Random;

public class ServiceImp implements Service{
	private String name;
	private int age;
	private Random ran;

	public ServiceImp() {
		System.out.println("ServiceImp() Created");
	}
	
	public ServiceImp(String name) {
		this.name = name;
	}
	
	public ServiceImp(String name, int age) {
		this.name = name;
		this.age = age;
	}
	
	@Override
	public void prn1() {
		System.out.printf("name: %s\n", name);
	}

	@Override
	public void prn2() {
		this.name = name;
		this.age = age;
	}

	@Override
	public void prn3() {
		
	}

}
